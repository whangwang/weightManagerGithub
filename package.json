{
  "name": "weightmanager",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build-css && npm run start-server",
    "start-server": "nodemon ./bin/www",
    "build-css": "node-sass ./public/stylesheets/scss/ -o ./public/stylesheets/css/",
    "build-css-watch": "node-sass -w ./public/stylesheets/scss/ -o ./public/stylesheets/css/"
  },
  "dependencies": {
    "async": "^2.6.2",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-session": "^1.16.1",
    "http-errors": "~1.6.2",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.0",
    "nodemailer": "^6.1.1",
    "nodemon": "^1.19.0"
  }
}
